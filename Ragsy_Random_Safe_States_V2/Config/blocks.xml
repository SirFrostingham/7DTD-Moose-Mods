<Random_Safe_States> 

	<!-- Removed to get rid of multi block DIM's spawning issue-->
	<remove xpath="/blocks/block[@name='cntGunSafeInsecure']"/>
	<remove xpath="/blocks/block[@name='cntGunSafe']"/>
	
	
